    <div class="col-r">
        <div class="inner reg-tel">
            <div class="title">
                通过手机注册
            </div>
            <div  class="form">
                <form method="post" id="" action="/web/reguser">
                    
                    <div class="form-item">
    <label for="UsernameIpt">手机号：</label>
    <span class="form-el">
        <input uui-type="require cellphone" type="text" name="username" id="UsernameIpt" value="" data-desc="" />
    </span>
</div>
                    <div class="form-item">
    <label >&nbsp;</label>
    <span class="form-el form-el-none">
        <span class="tel-valid-btn" action="" >免费获取短信验证码</span>
        <span class="tel-valid-error error"></span>
        <span class="tel-valid-hr" ></span>
    </span>
</div>
                    <div class="form-item">
    <label for="TelvIpt">短信验证码：</label>
    <span class="form-el"><input type="text" name="captcha" id="TelvIpt" value="" uui-type="require" /></span>
</div>
                    <div class="form-item">
    <label for="PasswordIpt">密码：</label>
    <span class="form-el">
        <input uui-type="require password" type="password" name="password" id="PasswordIpt" value="" data-desc="" />
    </span>
</div>
                    <div class="form-btn">
    <button type="submit">注册</button>
</div>
                    <div class="form-desc">
    <label for="ConfirmChb">
        <input type="checkbox" name="" value="1" id="ConfirmChb"  data-target="ConfirmChbSpan"/>
        <span class="checkbox " id="ConfirmChbSpan" ></span>
        我已阅读并接受
    </label>
</div>
                </form>
            </div>
        </div>
    </div>
</div>



